      icon: mdi:test-tube

    # Northern Maya Lowlands

    script.montesazules:
      friendly_name: 1. Bienvenida Experiencia Intel
      icon: mdi:audiobook

    script.ojodeliebre:
      friendly_name: 2. Juego Binario
      icon: mdi:audiobook

    script.lamichilia:
      friendly_name: 3. Intel en el Espacio
      icon: mdi:audiobook

    script.elcielo:
      friendly_name: 4. Los Muones
      icon: mdi:audiobook

    script.mapimi:
      friendly_name: 5. Hasta Pronto
      icon: mdi:audiobook

    script.siankaan:
      friendly_name: El Baile del Robot
