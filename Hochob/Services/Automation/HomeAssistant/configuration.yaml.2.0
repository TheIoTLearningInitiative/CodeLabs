homeassistant:
  # Name of the location where Home Assistant is running
  name: Home
  # Location required to calculate the time the sun rises and sets
  latitude: 20.35
  longitude: -102
  # Impacts weather/sunrise data (altitude above sea level in meters)
  elevation: 1674
  # metric for Metric, imperial for Imperial
  unit_system: metric
  # Pick yours from here: http://en.wikipedia.org/wiki/List_of_tz_database_time_zones
  time_zone: America/Mexico_City
  customize:

    # Language
    
    switch.language:
      friendly_name: Idioma English / Espanol
      icon: mdi:audiobook

    # Southern Maya Highlands : 1 Introduccion

    script.aguascalientes:
      friendly_name: 1.1 Bienvenidos Intel, Protocolo de Seguridad
      icon: mdi:audiobook

    # Southern Maya Highlands : 2 Video

    script.bajacalifornianorte:
      friendly_name: 2.1 Bienvenidos Guadalajara Design Center
      icon: mdi:audiobook
    script.bajacaliforniasur:
      friendly_name: 2.2 Lupe Robot Show
      icon: mdi:video
    script.michoacan:
      friendly_name: 2.3 Lupe Robot Foto Grupal
      icon: mdi:video      
    script.campeche:
      friendly_name: 2.4 Bienvenidos Recorrido
      icon: mdi:audiobook

    # Southern Maya Highlands : Corporate : 

    switch.guanajuato:
      friendly_name: Image Showcase
      icon: mdi:camera
    switch.guerrero:
      friendly_name: Video Showcase
      icon: mdi:camera

    # Southern Maya Highlands : Visitors Center

    switch.hidalgo:
      friendly_name: Image Showcase
      icon: mdi:camera

    # Southern Maya Highlands : Memories

    switch.queretaro:
      friendly_name: Image Showcase
      icon: mdi:camera
    switch.quintanaroo:
      friendly_name: Video Showcase
      icon: mdi:camera

    # Central Maya Lowlands

    script.acapulco:
      friendly_name: 1. Antes del inicio
      icon: mdi:audiobook
    script.cancun:
      friendly_name: 2. Intel
      icon: mdi:audiobook
    script.huatulco:
      friendly_name: 3. Intel GDC
      icon: mdi:audiobook
    script.mazatlan:
      friendly_name: 4. El microprocesador
      icon: mdi:audiobook
    script.puertovallarta:
      friendly_name: 5. Labs y Fabs
      icon: mdi:audiobook

    # Northern Maya Lowlands

    script.montesazules:
      friendly_name: 1. Bienvenida Experiencia Intel
      icon: mdi:audiobook

    script.ojodeliebre:
      friendly_name: 2. Juego Binario
      icon: mdi:audiobook

    script.lamichilia:
      friendly_name: 3. Intel en el Espacio
      icon: mdi:audiobook

    script.elcielo:
      friendly_name: 4. Los Muones
      icon: mdi:audiobook

    script.mapimi:
      friendly_name: 5. Hasta Pronto
      icon: mdi:audiobook

    # Robot Maya Lands

    script.resetall:
      friendly_name: Reset
      icon: mdi:lock-reset

    script.headright:
      friendly_name: Head Right
      icon: mdi:arrow-right-thick
    script.headleft:
      friendly_name: Head Left
      icon: mdi:arrow-left-thick

    script.rightup:
      friendly_name: Up
      icon: mdi:human-handsup
    script.rightdown:
      friendly_name: Down
      icon: mdi:human-handsup
    script.rightfold:
      friendly_name: Fold
      icon: mdi:gesture-swipe-up
    script.rightunfold:
      friendly_name: Unfold
      icon: mdi:gesture-swipe-up

    script.leftup:
      friendly_name: Up
      icon: mdi:human-handsup
    script.leftdown:
      friendly_name: Down
      icon: mdi:human-handsup
    script.leftfold:
      friendly_name: Fold
      icon: mdi:gesture-swipe-up
    script.leftunfold:
      friendly_name: Unfold
      icon: mdi:gesture-swipe-up

    script.moveright:
      friendly_name: Move Right
      icon: mdi:arrow-right-thick
    script.moveleft:
      friendly_name: Move Left
      icon: mdi:arrow-left-thick
    script.moveforward:
      friendly_name: Move Forward
      icon: mdi:arrow-up-thick
    script.movebackward:
      friendly_name: Move Backward
      icon: mdi:arrow-down-thick
    script.movestop:
      friendly_name: Move Stop
      icon: mdi:stop

# Enables the frontend
frontend:

http:
  # Uncomment this to add a password (recommended!)
  # api_password: PASSWORD
  # Uncomment this if you are using SSL or running in Docker etc
  # base_url: example.duckdns.org:8123

# Checks for available updates
updater:

# Discover some devices automatically
#discovery:

# Allows you to issue voice commands from the frontend in enabled browsers
conversation:

# Enables support for tracking state changes over time.
history:

# View all events in a logbook
logbook:

script:

  # Southern Maya Highlands

  aguascalientes:
    sequence:
      - service: shell_command.aguascalientes

  bajacalifornianorte:
    sequence:
      - service: shell_command.bajacalifornianorte

  bajacaliforniasur:
    sequence:
      - service: shell_command.bajacaliforniasur

  campeche:
    sequence:
      - service: shell_command.campeche

  michoacan:
    sequence:
      - service: shell_command.michoacan

  # Central Maya Lowlands

  acapulco:
    sequence:
      - service: shell_command.acapulco

  cancun:
    sequence:
      - service: shell_command.cancun

  huatulco:
    sequence:
      - service: shell_command.huatulco

  mazatlan:
    sequence:
      - service: shell_command.mazatlan

  puertovallarta:
    sequence:
      - service: shell_command.puertovallarta

  # Northern Maya Lowlands

  ojodeliebre:
    sequence:
      - service: shell_command.ojodeliebre

  montesazules:
    sequence:
      - service: shell_command.montesazules

  lamichilia:
    sequence:
      - service: shell_command.lamichilia

  elcielo:
    sequence:
      - service: shell_command.elcielo

  mapimi:
    sequence:
      - service: shell_command.mapimi
      
  # Robot Maya Lands

  resetall:
    sequence:
      - service: shell_command.resetall

  headright:
    sequence:
      - service: shell_command.headright

  headleft:
    sequence:
      - service: shell_command.headleft

  rightup:
    sequence:
      - service: shell_command.rightup

  rightdown:
    sequence:
      - service: shell_command.rightdown

  rightfold:
    sequence:
      - service: shell_command.rightfold

  rightunfold:
    sequence:
      - service: shell_command.rightunfold

  leftup:
    sequence:
      - service: shell_command.leftup

  leftdown:
    sequence:
      - service: shell_command.leftdown

  leftfold:
    sequence:
      - service: shell_command.leftfold

  leftunfold:
    sequence:
      - service: shell_command.leftunfold

  moveright:
    sequence:
      - service: shell_command.moveright

  moveleft:
    sequence:
      - service: shell_command.moveleft

  moveforward:
    sequence:
      - service: shell_command.moveforward

  movebackward:
    sequence:
      - service: shell_command.movebackward

  movestop:
    sequence:
      - service: shell_command.movestop

shell_command:

  # Southern Maya Highlands

  aguascalientes: /home/hochob/CodeLabs/Hochob/Applications/SouthernMayaHighlands/Aguascalientes.sh &
  bajacalifornianorte: /home/hochob/CodeLabs/Hochob/Applications/SouthernMayaHighlands/BajaCaliforniaNorte.sh &
  bajacaliforniasur: /home/hochob/CodeLabs/Hochob/Applications/SouthernMayaHighlands/BajaCaliforniaSur.sh &
  campeche: /home/hochob/CodeLabs/Hochob/Applications/SouthernMayaHighlands/Campeche.sh &
  michoacan: /home/hochob/CodeLabs/Hochob/Applications/SouthernMayaHighlands/Michoacan.sh &

  # Central Maya Lowlands

  acapulco: /home/hochob/CodeLabs/Hochob/Applications/CentralMayaLowlands/Acapulco.sh &
  cancun: /home/hochob/CodeLabs/Hochob/Applications/CentralMayaLowlands/Cancun.sh &
  huatulco: /home/hochob/CodeLabs/Hochob/Applications/CentralMayaLowlands/Huatulco.sh &
  mazatlan: /home/hochob/CodeLabs/Hochob/Applications/CentralMayaLowlands/Mazatlan.sh &
  puertovallarta: /home/hochob/CodeLabs/Hochob/Applications/CentralMayaLowlands/PuertoVallarta.sh &

  # Northern Maya Lowlands
  
  montesazules: /home/hochob/CodeLabs/Hochob/Applications/NorthernMayaLowlands/MontesAzules.sh &
  ojodeliebre: /home/hochob/CodeLabs/Hochob/Applications/NorthernMayaLowlands/OjoDeLiebre.sh &
  lamichilia: /home/hochob/CodeLabs/Hochob/Applications/NorthernMayaLowlands/LaMichilia.sh &
  elcielo: /home/hochob/CodeLabs/Hochob/Applications/NorthernMayaLowlands/ElCielo.sh &
  mapimi: /home/hochob/CodeLabs/Hochob/Applications/NorthernMayaLowlands/Mapimi.sh &
  
  # Robot Maya Lands
  
  resetall: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/resetall 1
  
  headleft: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/headleft 1
  headright: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/headright 1

  rightup: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/rightup
  rightdown: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/rightdown
  rightfold: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/rightfold
  rightunfold: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/rightunfold

  leftup: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/leftup
  lefttdown: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/leftdown
  leftfold: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/leftfold
  leftunfold: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/leftunfold
      
  moveright: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/moveright
  moveleft: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/moveleft
  moveforward: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/moveforward
  movebackward: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/movebackward
  movestop: /home/hochob/CodeLabs/Hochob/Services/Robot/Remote/Robot.sh lupe/movestop
 
switch:
  - platform: command_line
    switches:

      # Language
      
      language:
        command_on: "/home/hochob/CodeLabs/Hochob/Services/Language/Language.sh spanish &"
        command_off: "/home/hochob/CodeLabs/Hochob/Services/Language/Language.sh english"
 
      # Southern Maya Highlands: Corporate

      guanajuato:
        command_on: "/home/hochob/CodeLabs/Hochob/Applications/SouthernMayaHighlands/Guanajuato.sh &"
        command_off: "/home/hochob/CodeLabs/Hochob/Services/Kill/KillApp.sh eog"
      guerrero:
        command_on: "/home/hochob/CodeLabs/Hochob/Applications/SouthernMayaHighlands/Guerrero.sh &"
        command_off: "/home/hochob/CodeLabs/Hochob/Services/Kill/KillApp.sh vlc"

      # Southern Maya Highlands: Visitors Center

      hidalgo:
        command_on: "/home/hochob/CodeLabs/Hochob/Applications/SouthernMayaHighlands/Hidalgo.sh &"
        command_off: "/home/hochob/CodeLabs/Hochob/Services/Kill/KillApp.sh eog"

      # Southern Maya Highlands: Memories

      queretaro:
        command_on: "/home/hochob/CodeLabs/Hochob/Applications/SouthernMayaHighlands/Queretaro.sh &"
        command_off: "/home/hochob/CodeLabs/Hochob/Services/Kill/KillApp.sh eog"
      quintanaroo:
        command_on: "/home/hochob/CodeLabs/Hochob/Applications/SouthernMayaHighlands/QuintanaRoo.sh &"
        command_off: "/home/hochob/CodeLabs/Hochob/Services/Kill/KillApp.sh vlc"

group:

  default_view:
    view: yes
    entities:
      - group.language
      - group.introduction
      - group.video
      - group.corporate
      - group.visitorscenter
      - group.memories

  robotmayalands:
    view: yes
    name: The Robot
    entities:
      - group.robotmayalandsreset
      - group.robotmayalandshead
      - group.robotmayalandsarmright
      - group.robotmayalandsarmleft
      - group.robotmayalandslegs

  centralmayalowlands:
    view: yes
    name: The Beginning
    entities:
      - group.centralmayalowlandsstations

  northernmayalowlands:
    view: yes
    name: The Experience
    entities:
      - group.northernmayalowlandsstations

  language:
    name: Idioma
    entities:
      - switch.language

  introduction:
    name: 1 Introduccion
    entities:
      - script.aguascalientes

  video:
    name: 2 Video
    entities:
      - script.bajacalifornianorte
      - script.bajacaliforniasur
      - script.michoacan
      - script.campeche

  corporate:
    name: Corporate
    entities:
      - switch.guanajuato
      - switch.guerrero

  visitorscenter:
    name: Visitors Center
    entities:
      - switch.hidalgo

  memories:
    name: Memories
    entities:
      - switch.queretaro
      - switch.quintanaroo

  centralmayalowlandsstations:
    name: Stations
    entities:
      - script.acapulco
      - script.cancun
      - script.huatulco
      - script.mazatlan
      - script.puertovallarta
  
  northernmayalowlandsstations:
    name: Stations
    entities:
      - script.montesazules
      - script.ojodeliebre
      - script.lamichilia
      - script.elcielo
      - script.mapimi

  robotmayalandsreset:
    name: Reset
    entities:
      - script.resetall

  robotmayalandshead:
    name: Head
    entities:
      - script.headright
      - script.headleft

  robotmayalandsarmright:
    name: Arm Right
    entities:
      - script.rightup
      - script.rightdown
      - script.rightfold
      - script.rightunfold

  robotmayalandsarmleft:
    name: Arm Left
    entities:
      - script.leftup
      - script.lefttdown
      - script.leftfold
      - script.leftunfold
      
  robotmayalandslegs:
    name: Legs
    entities:
      - script.moveright
      - script.moveleft
      - script.moveforward
      - script.movebackward
      - script.movestop
 
