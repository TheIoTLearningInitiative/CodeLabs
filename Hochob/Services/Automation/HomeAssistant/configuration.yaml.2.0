        command_on: "~/CodeLabs/Hochob/Applications/MediaMayaLands/Cho.sh &"
        command_off: "~/CodeLabs/Hochob/Services/Kill/KillApp.sh eog"
      chuwen:
        command_on: "~/CodeLabs/Hochob/Applications/MediaMayaLands/Chuwen.sh &"
        command_off: "~/CodeLabs/Hochob/Services/Kill/KillApp.sh vlc"

      # Media Maya Lands: Camera

      kaab:
        command_on: "cvlc --no-video-title-show --fullscreen v4l2:///dev/video0 &"
        command_off: "pkill -9 vlc"

automation:

  - alias: TTS Clear Cache
    trigger:
      platform: time
      hours: '/24'
    condition:
      - condition: time
        weekday:
          - mon
    action:
      service: tts.clear_cache

  - alias: Home Assistant Start
    trigger:
      platform: homeassistant
      event: start
    action:
      - delay: '00:00:10'
      - service: tts.google_say
        data:
          message: 'Home Assistant has started'

  # Calendar Maya Lands

  - alias: pop
    initial_state: 'off'
